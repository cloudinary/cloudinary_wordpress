{
  "name": "cloudinary",
  "title": "Cloudinary",
  "author": "XWP",
  "license": "GPL-2.0+",
  "private": true,
  "homepage": "https://github.com/cloudinary/cloudinary_wordpress",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudinary/cloudinary_wordpress.git"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "build": "wp-scripts build",
    "deploy": "npm run build && grunt deploy && npm run release",
    "deploy-assets": "grunt deploy-assets",
    "dev": "wp-scripts start",
    "i18n": "grunt i18n",
    "lint": "npm-run-all lint:php lint:js lint:style",
    "lint:js": "wp-scripts lint-js",
    "lint:js:fix": "wp-scripts lint-js --fix",
    "lint:php": "composer lint",
    "lint:php:fix": "composer fix",
    "lint:style": "wp-scripts lint-style",
    "package": "./package/package.sh",
    "package:build": "npm run build && grunt package",
    "postinstall": "composer install",
    "readme": "composer readme",
    "release": "release-it --no-increment",
    "release:ci": "release-it --ci",
    "release:dry": "release-it --dry-run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.php": [
      "npm run lint:php"
    ],
    "*.js": [
      "npm run lint:js"
    ],
    "*.scss": [
      "npm run lint:style"
    ]
  },
  "dependencies": {
    "@tippyjs/react": "^4.2.5",
    "chart.js": "^3.5.0",
    "classnames": "^2.3.1",
    "codemirror": "^5.65.0",
    "dot-object": "^2.1.4",
    "file-size": "^1.0.0",
    "human-format": "^0.11.0",
    "loading-attribute-polyfill": "^1.5.4",
    "progressbar.js": "^1.1.1",
    "react": "^18.2.0",
    "tippy.js": "^6.3.1"
  },
  "devDependencies": {
    "@release-it/bumper": "^4.0.0",
    "@wordpress/api-fetch": "^3.23.1",
    "@wordpress/block-editor": "^5.1.2",
    "@wordpress/blocks": "^6.24.1",
    "@wordpress/browserslist-config": "^2.7.0",
    "@wordpress/components": "^11.1.5",
    "@wordpress/data": "^4.27.3",
    "@wordpress/element": "^2.20.3",
    "@wordpress/eslint-plugin": "^7.3.0",
    "@wordpress/i18n": "^3.16.0",
    "@wordpress/scripts": "^30.13.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "css-unicode-loader": "^1.0.3",
    "cssnano": "^5.0.2",
    "eslint": "^7.12.1",
    "eslint-plugin-jest": "^24.1.0",
    "file-loader": "^6.2.0",
    "grunt": "^1.5.2",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.6.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-shell": "^3.0.1",
    "grunt-text-replace": "^0.4.0",
    "grunt-wp-deploy": "^2.1.2",
    "grunt-wp-i18n": "^1.0.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "load-grunt-tasks": "^5.1.0",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^4.0.4",
    "prettier": "npm:wp-prettier@^2.0.5",
    "release-it": "15.6.0",
    "rtlcss-webpack-plugin": "^4.0.4",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.2.0"
  },
  "version": "3.2.14"
}
